{
  "name": "@royalnavy/react-component-library",
  "description": "A collection of react components and helpers to assist in building applications for the Royal Navy",
  "version": "0.1.1",
  "keywords": [
    "react",
    "components"
  ],
  "author": "NELSON",
  "contributors": [
    "98dfsj <98dfsj@royalnavy.io> (royalnavy.io)",
    "hxltrhuxze <hxltrhuxze@royalnavy.io> (royalnavy.io)",
    "antictor <antictor@royalnavy.io> (royalnavy.io)"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Royal-Navy/standards-toolkit"
  },
  "homepage": "https://github.com/Royal-Navy/standards-toolkit/tree/develop/packages/react-component-library#readme",
  "license": "MIT",
  "main": "./dist/bundle.js",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "webpack --config webpack/webpack.prod.js",
    "build-storybook": "build-storybook",
    "build-storybook-prod": "build-storybook -c .storybook -o public",
    "lint": "eslint src",
    "lint-staged": "lint-staged",
    "prepublishOnly": "npm run build",
    "storybook": "start-storybook -p 6006",
    "test": "jest --silent --no-cache",
    "watch": "webpack --config webpack/webpack.dev.js --watch"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "dependencies": {
    "react-autosuggest": "^9.4.3"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@emotion/core": "^10.0.10",
    "@emotion/styled": "^10.0.11",
    "@royalnavy/css-framework": "0.1.1",
    "@royalnavy/storybook-react-input-state": "0.1.1",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-info": "^5.0.11",
    "@storybook/addon-knobs": "^5.0.11",
    "@storybook/addon-links": "^5.0.11",
    "@storybook/addons": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "@storybook/theming": "^5.0.11",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-react-docgen": "^3.1.0",
    "clean-webpack-plugin": "^2.0.2",
    "css-loader": "^2.1.1",
    "emotion-theming": "^10.0.10",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-junit": "^1.0.1",
    "eslint-plugin-import": "^2.7.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "jest-junit": "^6.4.0",
    "lint-staged": "^8.1.6",
    "lodash": "^4.17.11",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "prop-types": "^15.7.2",
    "react-datepicker": "^2.5.0",
    "regenerator-runtime": "^0.13.2",
    "sass": "^1.20.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^10.0.1",
    "stylelint-webpack-plugin": "^0.10.5",
    "uuid": "^3.3.2",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-merge": "^4.2.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "react-docgen",
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "lint-staged": {
    "*.@(js)": "eslint"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./tests/setupTests.js"
    ],
    "testMatch": [
      "**/?(*.)(spec|test).js?(x)"
    ],
    "moduleNameMapper": {
      "^.+.(css|sass|scss)$": "identity-obj-proxy"
    }
  }
}
